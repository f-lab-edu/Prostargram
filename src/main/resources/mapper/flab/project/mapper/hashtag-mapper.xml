<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="flab.project.mapper.HashtagMapper">

  <select id="retrieveHashtagsIn" resultType="string">
    SELECT name FROM HASHTAGS
    <where>
      name IN
      <foreach item="hashtag" collection="hashtags" open="(" separator="," close=")">
        #{hashtag}
      </foreach>
    </where>
  </select>

  <insert id="insertAll">
    INSERT INTO HASHTAGS(name)
    <foreach item="hashtag" collection="hashtags" separator=",">
      VALUES(#{hashtag})
    </foreach>
  </insert>
</mapper>